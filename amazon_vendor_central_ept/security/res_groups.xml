<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">
        <record model="ir.module.category" id="module_amazon_vendor_central_ept">
            <field name="name">Amazon Vendor Central</field>
            <field name="description">Amazon Vendor Central Access Rights</field>
            <field name="sequence">1000</field>
        </record>
        <record model="res.groups" id="group_amazon_vendor_central_user">
            <field name="name">User</field>
            <field name="category_id" ref="module_amazon_vendor_central_ept"/>
            <field name="implied_ids"
                   eval="[(4,ref('sales_team.group_sale_salesman_all_leads')),
                   (4,ref('stock.group_tracking_lot')),(4,ref('account.group_account_invoice'))]"/>
        </record>
        <record model="res.groups" id="group_amazon_vendor_central_manager">
            <field name="name">Manager</field>
            <field name="category_id" ref="module_amazon_vendor_central_ept"/>
            <field name="implied_ids"
                   eval="[(4,ref('amazon_vendor_central_ept.group_amazon_vendor_central_user')),
                   (4,ref('stock.group_stock_manager')),(4,ref('sales_team.group_sale_manager')),
                   (4,ref('account.group_account_manager'))]"/>
            <field name="users" eval="[(4, ref('base.user_root')),(4, ref('base.user_admin'))]"/>
        </record>
        <record id="group_avc_packages" model="res.groups">
            <field name="name">Manage AVC Packages</field>
            <field name="category_id" ref="base.module_category_usability"/>
        </record>
    </data>
</odoo>
