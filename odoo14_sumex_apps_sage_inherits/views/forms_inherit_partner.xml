<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <record id="sumex_apps_sage_inherits_view_inherit_partner_search" model="ir.ui.view">
        <field name="name">sumex_apps_sage_inherits_view_inherit_partner_search</field>
        <field name="model">res.partner</field>
        <field name="inherit_id" ref="base.view_res_partner_filter"/>
        <field name="arch" type="xml">
            <xpath expr="//search" position="inside">
                <filter string="From import CSV of Sage - SÍ" name="_created_from_sumex_apps_imports_csv_of_sage_yes" domain="[('_created_from_sumex_apps_imports_csv_of_sage_contactos','!=',False)]"/>
                <filter string="From import CSV of Sage - NO" name="_created_from_sumex_apps_imports_csv_of_sage_no" domain="[('_created_from_sumex_apps_imports_csv_of_sage_contactos','=',False)]"/>
                <filter string="Sage Código cliente - SÍ Existe" name="_sage_field_codigocliente_si_existe" domain="[('_sage_field_codigocliente','!=',False)]"/>
                <filter string="Sage Código cliente - NO Existe" name="_sage_field_codigocliente_no_existe" domain="[('_sage_field_codigocliente','=',False)]"/>
                <field string="SAGE Código cliente" name="_sage_field_codigocliente" filter_domain="[('_sage_field_codigocliente','ilike',self)]"/>
            </xpath>
        </field>
    </record>

    <record id="sumex_apps_sage_inherits_view_inherit_partner_tree" model="ir.ui.view">
        <field name="name">sumex_apps_sage_inherits_view_inherit_partner_tree</field>
        <field name="model">res.partner</field>
        <field name="inherit_id" ref="base.view_partner_tree"/>
        <field name="arch" type="xml">

            <field name='display_name' position="after">
                <field name="_created_from_sumex_apps_imports_csv_of_sage_contactos" readonly="1" optional="hide"/>
            </field>

        </field>
    </record>

