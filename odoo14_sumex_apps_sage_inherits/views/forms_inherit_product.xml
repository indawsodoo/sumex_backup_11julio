<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <record id="sumex_apps_sage_inherits_view_inherit_product_search" model="ir.ui.view">
        <field name="name">sumex_apps_sage_inherits_view_inherit_product_search</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="product.product_template_search_view"/>
        <field name="arch" type="xml">
            <xpath expr="//search" position="inside">
                <filter string="From import CSV of Sage - SÍ" name="_created_from_sumex_apps_imports_csv_of_sage_yes" domain="[('_created_from_sumex_apps_imports_csv_of_sage','!=',False)]"/>
                <filter string="From import CSV of Sage - NO" name="_created_from_sumex_apps_imports_csv_of_sage_no" domain="[('_created_from_sumex_apps_imports_csv_of_sage','=',False)]"/>
                <filter string="Sage Número pedido - SÍ Existe" name="_sage_field_codigoarticulo_existe" domain="[('_sage_field_codigoarticulo','!=',False)]"/>
                <filter string="Sage Número pedido - NO Existe" name="_sage_field_codigoarticulo_existe" domain="[('_sage_field_codigoarticulo','=',False)]"/>
                <field string="SAGE Código artículo" name="_sage_field_codigoarticulo" filter_domain="[('_sage_field_codigoarticulo','ilike',self)]"/>
            </xpath>
        </field>
    </record>

    <record id="sumex_apps_sage_inherits_view_inherit_product_tree" model="ir.ui.view">
        <field name="name">sumex_apps_sage_inherits_view_inherit_product_tree</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="product.product_template_tree_view"/>
        <field name="arch" type="xml">

            <field name='categ_id' position="after">
                <field name="_created_from_sumex_apps_imports_csv_of_sage" readonly="1" optional="show"/>
            </field>

        </field>
    </record>

