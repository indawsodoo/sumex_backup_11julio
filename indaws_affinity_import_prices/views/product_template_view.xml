<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="product_template_only_form_view_inherit_indaws_custom" model="ir.ui.view">
        <field name="name">product.template.product.form.indaws.custom</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="product.product_template_form_view"/>
        <field name="arch" type="xml">
            <xpath expr="//page[@name='purchase']" position="after">
                <page string="Code Proveedor">
                        <field name="code_vendor_ids">
                            <tree string="Code Proveedor" create="false" edit="false" editable="bottom">
                                <field name='partner_id'/>
                                <field name='vendor_code'/>
                                <field name='product_code'/>
                            </tree>
                        </field>
                </page>
            </xpath>
        </field>
    </record>
</odoo>
